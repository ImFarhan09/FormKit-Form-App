ration Form</h1>\r\n        <div class=\"border border-1 border-grey p-2 mt-4 rounded w-25\">\r\n        <FormKit type=\"form\" submit-label=\"Register\" v-model=\"userData\" @submit=\"onSubmit\">\r\n            <FormKit \r\n                type=\"text\" \r\n                label=\"First Name\" \r\n                placeholder=\"Jack\" \r\n                name=\"name-first\" \r\n                validation=\"required\"\r\n            />\r\n            <FormKit \r\n                type=\"text\" \r\n                label=\"Last Name\" \r\n                placeholder=\"Shukla\" \r\n                name=\"name-last\" \r\n                validation=\"required\"\r\n            />\r\n            <!-- Regex for an indian mobile number with +91 -->\r\n            <!-- validation=\"required|length:13|matches:/[\\+91]?[6-9][0-9]{9}/\" -->\r\n            <FormKit \r\n                type=\"number\" \r\n                label=\"Mobile\" \r\n                placeholder=\"9876543210\" \r\n                name=\"mobile\" \r\n                help=\"Enter your 10-digit number\" \r\n                validation=\"required|length:10|matches:/[6-9][0-9]{9}/\"\r\n            />\r\n            <FormKit \r\n                type=\"email\" \r\n                label=\"Email\" \r\n                placeholder=\"example@gmail.com\" \r\n                name=\"email\" \r\n                validation=\"required|email\"\r\n            />\r\n            <FormKit \r\n                type=\"password\" \r\n                label=\"Password\" \r\n                placeholder=\"********\" \r\n                name=\"password\"\r\n                validation=\"required|length:8\"\r\n            />\r\n            <FormKit \r\n                type=\"password\" \r\n                label=\"Confirm password\" \r\n                placeholder=\"********\" \r\n                name=\"password_confirm\" \r\n                help=\"Re-enter same password\" \r\n                validation=\"required|confirm\"\r\n            />\r\n            <FormKit \r\n                type=\"date\" \r\n                label=\"Date of Birth\" \r\n                name=\"date_of_birth\" \r\n                validation=\"required|date_before:2010-01-01\"\r\n            />\r\n            <FormKit \r\n                type=\"select\" \r\n                label=\"Gender\" \r\n                name=\"gender\"\r\n                :options=\"options\" \r\n                validation=\"required\"\r\n                validation-label=\"Gender\"\r\n            />\r\n            <FormKit \r\n                type=\"checkbox\" \r\n                label=\"Skills\" \r\n                name=\"skills\"\r\n                :options=\"soptions\" \r\n                validation=\"+min:1\"\r\n            />\r\n            <FormKit \r\n                type=\"radio\" \r\n                label=\"Degree\" \r\n                name=\"degree\"\r\n                :options=\"doptions\" \r\n                validation=\"required\"\r\n            />\r\n            <FormKit \r\n                type=\"checkbox\" \r\n                label=\"I accept \" \r\n                name=\"acceptance\"\r\n                validation=\"required\"\r\n                validation-label=\"Acceptance\"\r\n            />\r\n        </FormKit>\r\n    </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n"],"mappings":"AACA,OAAO,yBAAyB;AAEhC,eAAe;EACXA,IAAI,EAAC,eAAe;EACpBC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,OAAO,EAAE;QACL,EAAE,EAAE,QAAQ;QACZC,IAAI,EAAC,MAAM;QACXC,MAAM,EAAC,QAAQ;QACfC,KAAK,EAAC;MACV,CAAC;MACDC,QAAQ,EAAE;QACNC,KAAK,EAAC,aAAa;QACnBC,OAAO,EAAC;MACZ,CAAC;MACDC,QAAQ,EAAE;QACNC,KAAK,EAAC,SAAS;QACfC,OAAO,EAAC,WAAW;QACnBC,GAAG,EAAC;MACR,CAAC;MACDC,QAAQ,EAAE,CAAC;IACf;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAE;MACN,MAAMC,MAAK,GAAIC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACL,QAAQ,CAAC;MAC5C,MAAMM,SAAS,GAAEF,IAAI,CAACG,KAAK,CAACJ,MAAM,CAAC;MACnCK,OAAO,CAACC,GAAG,CAACH,SAAS,CAAC;IAC1B;EAEJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}